{
	"$schema": "https://turbo.build/schema.json",
	"extends": ["//"],
	"tags": ["internal"],
	"tasks": {
		"prepare": {
			"dependsOn": ["@bank-kit/utils#build"],
			"outputs": ["public/**"]
		},
		"generate:workspace": {
			"cache": true,
			"inputs": ["scripts/generate-workspace.ts"],
			"outputs": ["generated/**"]
		},
		"build": {
			"dependsOn": [],
			"outputs": ["dist/**"]
		}
	}
}
